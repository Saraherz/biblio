@article{ WOS:000434579600003,
Author = {Papadavid, George and Perdikou, Skevi and Hadjimitsis, Michalakis and
   Hadjimitsis, Diofantos},
Title = {Remote Sensing Applications for Planning Irrigation Management. The Use
   of SEBAL Methodology for Estimating Crop Evapotranspiration in Cyprus},
Journal = {ENVIRONMENTAL AND CLIMATE TECHNOLOGIES},
Year = {2012},
Volume = {9},
Number = {1},
Pages = {17-21},
Month = {DEC},
Abstract = {Water allocation to crops has always been of great importance in the
   agricultural process. In this context, and under the current conditions,
   where Cyprus is facing a severe drought the last five years, the purpose
   of this study is basically to estimate the needed crop water
   requirements for supporting irrigation management and monitoring
   irrigation on a systematic basis for Cyprus using remote sensing
   techniques. The use of satellite images supported by ground measurements
   has provided quite accurate results. Intended purpose of this paper is
   to estimate the Evapotranspiration (ET) of specific crops which is the
   basis for irrigation scheduling and establish a procedure for monitoring
   and managing irrigation water over Cyprus, using remotely sensed data
   from Landsat TM/ETM+ and a sound methodology used worldwide, the Surface
   Energy Balance Algorithm for Land (SEBAL).},
Publisher = {SCIENDO},
Address = {BOGUMILA ZUGA 32A, WARSAW, MAZOVIA, POLAND},
Type = {Article},
Language = {English},
Affiliation = {Papadavid, G (Corresponding Author), 7 Dionysou Str, CY-8041 Paphos, Cyprus.
   Papadavid, George, Agr Res Inst Cyprus, Aglandjia, Cyprus.
