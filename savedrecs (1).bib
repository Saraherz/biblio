@article{ WOS:000287432900005,
Author = {El Nahry, A. H. and Ali, R. R. and El Baroudy, A. A.},
Title = {An approach for precision farming under pivot irrigation system using
   remote sensing and GIS techniques},
Journal = {AGRICULTURAL WATER MANAGEMENT},
